    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Web Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "10001",
                "--dart-define=LENRA_SERVER_URL=http://localhost:4000"
            ]
        },
        {
            "name": "Run AEmul Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "release",
            "type": "dart",
            "args": [
                "-d",
                "emulator-5554",
                "--web-port",
                "10001",
                "--dart-define=LENRA_SERVER_URL=http://192.168.1.79:4000"
            ]
        }
    ]
}